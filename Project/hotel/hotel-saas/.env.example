# Environment Variables Template for HotelEase
# Copy this file to .env and fill in your values

# Base URL for your web app
REACT_APP_BASE_URL=https://your-domain.com

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
REACT_APP_FIREBASE_APP_ID=your-app-id

# API URL (if you have a backend)
REACT_APP_API_URL=https://api.your-domain.com

# Feature Flags
REACT_APP_ENABLE_BOOKING=true
REACT_APP_ENABLE_USER_REVIEWS=true

# Other Configuration
REACT_APP_HOTEL_NAME=HotelEase
REACT_APP_SUPPORT_EMAIL=support@yourdomain.com 